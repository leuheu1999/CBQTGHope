@using Module.Framework.Common;
@{
    var user = LoginManager.GetCurrentUser();
    var admin = LoginManager.CheckAdmin();
    var modelParam = new Business.Entities.Domain.QTG_SearchQuyenTacGiaParam();
    var modelList = new List<Business.Entities.Domain.QTG_SearchQuyenTacGiaMap>();
}
<div class="modal fade custom-margin-form-group" id="dialogSearchQuyenTacGia" aria-hidden="false" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header h-25">
                <h5 class="modal-title">Thông tin quyền tác giả </h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    @Html.Partial("DialogSearchQuyenTacGia/Search", modelParam)
                </div>
                <div class="table-responsive mt-3 sh-list-quyentacgia" id="result-searchquyentacgia">
                    @Html.Partial("DialogSearchQuyenTacGia/List", modelList)
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary btn-sm" data-dismiss="modal"><i class="fas fa-times mr-2"></i> Đóng</button>
            </div>
        </div>
    </div>
</div>
<script>
    $(document).ready(function () {
        if (Common.Empty(Common.QTG_QuyenTacGia_SearchQuyenTacGia)) {
            Common.QTG_QuyenTacGia_SearchQuyenTacGia = new QTG_QuyenTacGia_SearchQuyenTacGia({
                Url: {
                    SearchQuyenTacGia: '@Url.Action("SearchQuyenTacGia", "QTG_QuyenTacGia")',
                },
            });
        }
        else {
            Common.QTG_QuyenTacGia_SearchQuyenTacGia.RegisterEvent();
        }
    });
</script>